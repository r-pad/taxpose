{
    "editor.formatOnSave": true,
    "python.formatting.provider": "black",
    "python.linting.pylintArgs": [
        "--generated-members",
        "torch.*"
    ],
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "files.exclude": {
        "data/*": true,
    },
    "python.linting.enabled": false
}
